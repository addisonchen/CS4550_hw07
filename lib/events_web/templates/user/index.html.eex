<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <%= for user <- @users do %>
        <tr>
          <td><span><%= link "#{user.name}", to: Routes.user_path(@conn, :show, user) %></span></td>
          <td style="width: 20px;"></td>
          <td><%= user.email %></td>
        </tr>
    <% end %>
  </tbody>
</table>